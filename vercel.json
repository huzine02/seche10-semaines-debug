{
  "rewrites": [
    { "source": "/blog/:slug", "destination": "/blog/:slug/index.html" },
    { "source": "/((?!blog/)(?!assets/)(?!blog-images/)(?!favicon)(?!manifest)(?!sw\\.js)(?!sitemap)(?!robots)(?!google).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/blog/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, s-maxage=604800" }
      ]
    }
  ]
}
